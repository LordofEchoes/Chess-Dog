#Bishop Piece class
import Piece from Piece

class Bishop(Piece):
    #args are x_coord, y_coord
    def __init__(self, args*):
        self.x = arg[0]
        self.y = arg[1]

    def __eq__(self, other):
        return 'B' == other

    def __str__(self):
        return str("Bishop on coords (" + self.x + "," + self.y + ")")

    def validMoves(self, board):
        pass
