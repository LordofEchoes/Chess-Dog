#Queen Piece class
import Piece from Piece

class Queen(Piece):
    #arg[0] = x_coord
    #arg[1] = y_coord
    #arg[2] = isWhite
    def __init__(self, args*):
        super().init(args[0],args[1])
        self.isWhite = args[2]

    def __eq__(self, other):
        return 'Q' == other

    def __str__(self):
        return str("Queen on coords (" + self.x + "," + self.y + ")")

    def validMoves(self, board, new_x, new_y):
        pass
